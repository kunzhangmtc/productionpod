<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		title="Confirm Log Off!"
		type="Message"
		class="sapUiPopupWithPadding">
		<content>
			<Text text="Are you sure you want to log off the operators?"/>
		</content>
			<beginButton>
				<Button text="Yes" press="handleConfirmLogOff"/>
			</beginButton>
			<endButton>
				<Button text="Cancel" press="handleCancelLogOff"/>			
			</endButton>
	</Dialog>
</core:FragmentDefinition>